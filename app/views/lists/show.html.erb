<h4>タイトル</h4>
<p><%= @list.title %></p>
<h4>本文</h4>
<p><%= @list.body %></p>
<h4>画像</h4>
<% if @list.image.attached? %>
<p><%= image_tag @list.image,size: "200x200"%></p>
<% else %>
<p><%= image_tag 'no_image',size: "200×200"%></p>
<% end %>
<%= link_to "編集",edit_list_path(@list.id) %>
<%= link_to "削除", list_path(@list.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>